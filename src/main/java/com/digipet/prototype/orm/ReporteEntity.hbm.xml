<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.digipet.prototype.orm.ReporteEntity" table="REPORTE" schema="digipet">
        <id name="idReporte">
            <column name="Id_reporte" sql-type="int(11)"/>
        </id>
        <property name="cantidadCaca">
            <column name="Cantidad_caca" sql-type="int(11)"/>
        </property>
        <property name="cantidadOrines">
            <column name="Cantidad_orines" sql-type="int(11)"/>
        </property>
        <property name="cantidadJuegos">
            <column name="Cantidad_juegos" sql-type="int(11)"/>
        </property>
        <property name="distancia">
            <column name="Distancia" sql-type="decimal(2,1)" precision="2" scale="1"/>
        </property>
        <property name="detalles">
            <column name="Detalles" sql-type="varchar(300)" length="300"/>
        </property>
        <property name="idSolicitud">
            <column name="Id_solicitud" sql-type="int(11)"/>
        </property>
    </class>
</hibernate-mapping>