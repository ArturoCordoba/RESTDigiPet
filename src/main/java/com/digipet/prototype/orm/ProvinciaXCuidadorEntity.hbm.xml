<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.digipet.prototype.orm.ProvinciaXCuidadorEntity" table="PROVINCIA_X_CUIDADOR" schema="digipet">
        <composite-id mapped="true" class="com.digipet.prototype.orm.ProvinciaXCuidadorEntityPK">
            <key-property name="idCuidador">
                <column name="Id_cuidador" sql-type="int(11)"/>
            </key-property>
            <key-property name="idProvincia">
                <column name="Id_provincia" sql-type="int(11)"/>
            </key-property>
        </composite-id>
    </class>
</hibernate-mapping>