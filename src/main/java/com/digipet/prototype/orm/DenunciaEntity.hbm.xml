<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.digipet.prototype.orm.DenunciaEntity" table="DENUNCIA" schema="digipet">
        <id name="idDenuncia">
            <column name="Id_denuncia" sql-type="int(11)"/>
        </id>
        <property name="idCliente">
            <column name="Id_cliente" sql-type="int(11)"/>
        </property>
        <property name="idCuidador">
            <column name="Id_cuidador" sql-type="int(11)"/>
        </property>
        <property name="descripcion">
            <column name="Descripcion" sql-type="varchar(300)" length="300"/>
        </property>
    </class>
</hibernate-mapping>