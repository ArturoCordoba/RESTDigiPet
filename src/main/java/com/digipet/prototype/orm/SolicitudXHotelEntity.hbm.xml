<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.digipet.prototype.orm.SolicitudXHotelEntity" table="SOLICITUD_X_HOTEL" schema="digipet">
        <composite-id mapped="true" class="com.digipet.prototype.orm.SolicitudXHotelEntityPK">
            <key-property name="idSolicitud">
                <column name="Id_solicitud" sql-type="int(11)"/>
            </key-property>
            <key-property name="idHotel">
                <column name="Id_hotel" sql-type="int(11)"/>
            </key-property>
        </composite-id>
    </class>
</hibernate-mapping>