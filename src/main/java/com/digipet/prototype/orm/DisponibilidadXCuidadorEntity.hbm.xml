<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.digipet.prototype.orm.DisponibilidadXCuidadorEntity" table="DISPONIBILIDAD_X_CUIDADOR" schema="digipet">
        <id name="idDisponibilidad">
            <column name="Id_disponibilidad" sql-type="int(11)"/>
        </id>
        <property name="idCuidador">
            <column name="Id_cuidador" sql-type="int(11)"/>
        </property>
        <property name="fechaHoraInicio">
            <column name="Fecha_hora_inicio" sql-type="datetime"/>
        </property>
        <property name="fechaHoraFinal">
            <column name="Fecha_hora_final" sql-type="datetime"/>
        </property>
    </class>
</hibernate-mapping>